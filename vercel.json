{
    "projects": {
      "server": {
        "outputDirectory": "dist"
      },
      "front": {
        "outputDirectory": "out",
        "framework": "nextjs"
      }
    },
    "builds": [
      {
        "src": "front/package.json",
        "use": "@vercel/next",
        "config": {
          "distDir": "out"
        }
      },
      {
        "src": "server/package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/front/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "/server/api/$1"
      }
    ]
  }
  